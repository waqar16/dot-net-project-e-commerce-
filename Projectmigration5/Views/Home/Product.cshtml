
@{
    ViewBag.Title = "Product";
    Layout = null;
}
<head>

    <title>KES: Wlcome to Khan Electric Store</title>
    <link href="https://fonts.googleapis.com/css?family=Raleway"
          rel="stylesheet" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css"
          rel="stylesheet"
          integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi"
          crossorigin="anonymous" />
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" />
    <link href="~/Content/css/style.css" rel="stylesheet" />
    <link href="~/Content/css/Productstyle.css" rel="stylesheet" />
    <link href="~/Content/css/footer.css" rel="stylesheet" />

</head>

<body>


    <!-- topbar -->
    <div class="top">
        <div class="top-bar">
            <div class="social-icons-btn">
                <ul>
                    <li>
                        <a href=""><img src="/Content/images/socials/facebook.svg" alt="" /></a>
                    </li>
                    <li>
                        <a href=""><img src="/Content/images/socials/instagram.svg" alt="" /></a>
                    </li>
                    <li>
                        <a href=""><img src="/Content/images/socials/whatsapp.svg" alt="" /></a>
                    </li>
                </ul>
            </div>
            @* <div class="login-signup-btn">
                    <ul>
                        <li><a href="login.html">Login</a></li>
                        <li><a href="signup.html">Register</a></li>
                    </ul>
                </div>*@
        </div>
    </div>
    <!-- navbar -->
    <div id="container">
        <nav>
            <div id="logo">
                <img src="~/Content/images/logo/logo.jpg" />

            </div>
            <ul>
                <li>@Html.ActionLink("Home", "Index", "Home")</li>
                <li>@Html.ActionLink("Product", "Product", "Home")</li>
                <li>@Html.ActionLink("Order", "Order", "Home")</li>
                <li>@Html.ActionLink("About", "About", "Home")</li>
                <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
            </ul>
        </nav>
    </div>

    <div class="brand_color">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="titlepage">
                        <h2>our product</h2>
                    </div>
                </div>
            </div>
        </div>

    </div>
    <!-- our product -->


    <div class="product">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="title">

                        <span>We package the products with best services to make you a happy customer.</span>
                    </div>
                </div>
            </div>
        </div>
    </div>

    @{ Html.RenderAction("pvProducts", "Home"); }

    <!-- Site footer -->
    <footer class="site-footer">
        <div class="container">
            <div class="row">
                <div class="col-sm-12 col-md-6">
                    <h6>About</h6>
                    <p class="text-justify">
                        Khan Electric Store <i> ALl Electric Items in Your Fingertips </i>
                        is an electric products store focuses on selling items at the low price online.
                        We sell all kinds of famous brands of electrical wires, cables, pipes, conduits,
                        switches, sockets, wiring accessories, lights,, LED lights, LED bulbs, Ceiling lights
                        emergency lights, tools and replacement parts for electrical equipment at cheaper
                        prices.
                    </p>
                </div>

                <div class="col-xs-6 col-md-3">
                    <h6>Policy</h6>
                    <ul class="footer-links">
                        <li><a href="TermsandConditions.html">Return Policy</a></li>
                        <li>@Html.ActionLink("TermsandCondition", "TermsandCondition", "Home")</li>
                        <li><a href="TermsandConditions.html">Warranty</a></li>
                        <li><a href="TermsandConditions.html">Privacy</a></li>
                        <li><a href="TermsandConditions.html">Price Beating</a></li>
                    </ul>
                </div>

                <div class="col-xs-6 col-md-3">
                    <h6>Quick Links</h6>
                    <ul class="footer-links">
                        <li>@Html.ActionLink("About", "About", "Home")</li>
                        <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                        <li>@Html.ActionLink("Product", "Product", "Home")</li>
                        <li><a href="TermsandConditions.html">Privacy Policy</a></li>
                        <li><a href="">Help</a></li>
                    </ul>
                </div>
            </div>
            <hr>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-sm-6 col-xs-12">
                    <p class="copyright-text">
                        Copyright &copy; 2017 All Rights Reserved by
                        <a href="#">Khan Electric Store</a>.
                    </p>
                </div>

            </div>
        </div>
    </footer>


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3"
            crossorigin="anonymous"></script>
    <script src="~/Content/javascript/main.js"></script>
</body>


